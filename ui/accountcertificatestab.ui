<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.10"/>
  <template class="AccountCertificatesTab" parent="GtkBox">
    <property name="visible">True</property>
    <property name="orientation">vertical</property>
    <property name="spacing">10</property>
    <property name="border_width">10</property>
    <child>
      <object class="GtkFrame" id="frame_certificates">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <!-- <property name="label_xalign">0</property> -->
        <property name="shadow_type">none</property>
        <child type="label">
          <object class="GtkLabel" id="label_allowed_contacts">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="label" translatable="yes">Allowed Contacts</property>
            <attributes>
              <attribute name="weight" value="bold"/>
            </attributes>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="hbox_certificates">
            <property name="visible">True</property>
            <property name="orientation">vertical</property>
            <property name="margin_left">10</property>
            <property name="margin_top">10</property>
            <property name="spacing">10</property>
            <child>
              <object class="GtkScrolledWindow" id="scrolledwindow_certificates">
                <property name="visible">True</property>
                <property name="shadow_type">in</property>
                <!-- <property name="min-content-height">225</property> -->
                <property name="hscrollbar_policy">never</property>
                <property name="vscrollbar_policy">never</property>
                <child>
                  <object class="GtkTreeView" id="treeview_allowed_certificates">
                    <property name="visible">True</property>
                  </object>
                </child>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">True</property>
              </packing>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
